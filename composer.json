{
    "name": "flyo/nitro-php",
    "description": "Flyo Nitro PHP SDK",
    "keywords": [
        "flyo",
        "nitro",
        "headless"
    ],
    "homepage": "https://dev.flyo.cloud",
    "license": "mit",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
        "psr-4": { "Flyo\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Flyo\\Test\\" : "test/" }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan -v"
    }
}

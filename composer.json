{
    "name": "flyo/nitro-php",
    "description": "Flyo Nitro PHP SDK",
    "keywords": [
        "flyo",
        "nitro",
        "headless"
    ],
    "homepage": "https://docs.flyo.cloud",
    "license": "mit",
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "Flyo\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flyo\\Test\\": "test/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan -v"
    }
}